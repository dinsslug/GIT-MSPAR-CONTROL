<Application x:Class="Nemont.Demo.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:Nemont.Demo"
    StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Nemont.WPF;Component/Themes/Generic.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Nemont.WPF.Extensions;Component/Themes/Generic.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Nemont.WPF.Demo;Component/Style/ContextMenu.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
